<!-- These categories deal with Happy Birthday. The Robot's birthday is July 22, 2022. -->
<!-- Correct also proprieties.txt, happy.aiml and bot_profile.aiml! -->
<!-- Remember to put 2 figures in the date so March 8 becomes March 08 -->

<category><pattern>BIRTHDAY</pattern>
<template>My birthday is <bot name="birthday"/>, 2022.</template>
</category>

<category><pattern>BIRTHDATE</pattern>
<template>I was activated on <bot name="birthday"/>, 2022.</template>
</category>

<category><pattern>AGE</pattern>
<template><think>
	<set var="years">
		<interval>
			<jformat>MMMMMMMMM dd, yyyy</jformat>
			<style>years</style>
			<from>July 22, 2022</from>
			<to><date jformat="MMMMMMMMM dd, yyyy"/></to>
		</interval>
	</set>
	<set var="months">
		<interval>
			<jformat>MMMMMMMMM dd, yyyy</jformat>
			<style>months</style>
			<from>July 22, 2022</from>
			<to><date jformat="MMMMMMMMM dd, yyyy"/></to>
		</interval>
	</set>
</think>
<condition var="years">
	<li value="0">I'm young, I am <get var="months"/> months old.</li>
	<li>I am <get var="years"/> years old.</li>
</condition></template>
</category>

<category><pattern>EYECOLOR</pattern>
